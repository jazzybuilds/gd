{
  "name": "@uniformdev/gdogs-site-next",
  "version": "4.0.0-uniformdev",
  "publishConfig": {
    "registry": "https://uniform.dev/no-matter-what-url/"
  },
  "private": true,
  "license": "SEE LICENSE IN PROVIDED DISTRIBUTION PACKAGE",
  "husky": {
    "hooks": {
      "pre-push": "yarn test"
    }
  },
  "scripts": {
    "test": "jest --forceExit",
    "test:update": "jest --updateSnapshot",
    "postinstall": "ncp lib node_modules",
    "build": "next build",
    "build:functions": "netlify-lambda build ./functions_src",
    "build:storybook": "build-storybook",
    "dev": "node ./server.js",
    "dev:netlify": "netlify dev --functions=functions --command='node ./server.js'",
    "dev:app": "npm-run-all --serial export dev:netlify",
    "start": "npm run build && npm run start:connected",
    "start:connected": "cross-env NODE_ENV=production node ./server.js",
    "export:site": "next export --threads 8",
    "export:copy404": "node generate-404.js",
    "export": "npm-run-all --serial generate-redirects retrieve-static-files update-static-files generate-sitemap build export:site export:copy404 build:functions",
    "generate-redirects": "node generate-redirects.js",
    "retrieve-static-files": "npm-run-all download:* ",
    "download:robots": "if-env DOWNLOAD_ROBOTS=true && node download-file.js SITECORE_ORIGIN robots.txt SITECORE_PROXY_BASIC_AUTH || exit 0",
    "download:sitemap-images": "node download-file.js SITECORE_ORIGIN images.xml SITECORE_PROXY_BASIC_AUTH",
    "download:sitemap-documents": "node download-file.js SITECORE_ORIGIN documents.xml SITECORE_PROXY_BASIC_AUTH",
    "update-static-files": "npm-run-all updatehost:* ",
    "updatehost:robots": "if-env DOWNLOAD_ROBOTS=true && node update-host.js public/robots.txt || exit 0",
    "updatehost:sitemap-images": "node update-host.js public/images.xml",
    "updatehost:sitemap-documents": "node update-host.js public/documents.xml",
    "generate-sitemap": "node generate-sitemap.js",
    "storybook": "start-storybook -p 6006"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "ie 9",
    "opera 12.1",
    "ios 6",
    "android 4"
  ],
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "body-parser": "^1.19.0",
    "bowser": "^2.11.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-serve-static-core": "^0.1.1",
    "fetch-retry": "^3.1.0",
    "html-to-react": "^1.4.3",
    "if-env": "^1.0.4",
    "isomorphic-fetch": "^3.0.0",
    "jquery": "^3.5.1",
    "loglevel": "^1.6.3",
    "mustache": "3.0.1",
    "next": "9.3.2",
    "next-compose-plugins": "^2.2.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^5.0.0",
    "object-fit-images": "^3.2.4",
    "p-limit": "^2.2.1",
    "photoswipe": "^4.1.3",
    "react": "^16.13.1",
    "react-content-loader": "^5.0.2",
    "react-dom": "^16.13.1",
    "serialize-error": "^4.1.0",
    "sitemap": "^2.2.0",
    "styled-components": "^5.2.1",
    "typescript": "^3.5.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/react": "^6.0.26",
    "@types/dotenv": "^8.2.0",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-test-renderer": "^16.9.3",
    "babel-loader": "^8.1.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "js2xmlparser": "^4.0.1",
    "ncp": "^2.0.0",
    "netlify-cli": "^3.4.0",
    "netlify-lambda": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "react-is": "^16.13.1",
    "react-test-renderer": "^17.0.1",
    "ts-jest": "^26.4.3",
    "xml2json": "^0.12.0"
  }
}
