{
  "name": "@uniformdev/gdogs-site-next",
  "version": "4.0.0-uniformdev",
  "publishConfig": {
    "registry": "https://uniform.dev/no-matter-what-url/"
  },
  "private": true,
  "license": "SEE LICENSE IN PROVIDED DISTRIBUTION PACKAGE",
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "scripts": {
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "postinstall": "ncp lib node_modules",
    "build": "next build",
    "build:storybook": "build-storybook",
    "dev": "node ./server.js",
    "start": "npm run build && npm run start:connected",
    "start:connected": "cross-env NODE_ENV=production node ./server.js",
    "export:site": "next export --threads 8",
    "export": "npm-run-all --serial generate-redirects build export:site",
    "generate-redirects": "node generate-redirects.js",
    "storybook": "start-storybook -p 6006"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-serve-static-core": "^0.1.1",
    "fetch-retry": "^3.1.0",
    "html-to-react": "^1.4.3",
    "isomorphic-unfetch": "^3.0.0",
    "loglevel": "^1.6.3",
    "next": "9.3.2",
    "next-compose-plugins": "^2.2.0",
    "node-fetch": "^2.6.0",
    "p-limit": "^2.2.1",
    "react": "^16.13.1",
    "react-content-loader": "^5.0.2",
    "react-dom": "^16.13.1",
    "serialize-error": "^4.1.0",
    "sitemap": "^2.2.0",
    "typescript": "^3.5.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/react": "^6.0.26",
    "@types/dotenv": "^8.2.0",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-test-renderer": "^16.9.3",
    "babel-loader": "^8.1.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "react-is": "^16.13.1",
    "react-test-renderer": "^17.0.1",
    "ts-jest": "^26.4.3",
    "xml2json": "^0.12.0"
  }
}
